# Staging Environment Variables for Catalyst Frontend

# API Configuration
VITE_API_BASE_URL=https://api-staging.catalyst.example.com/api

# WebSocket Configuration
VITE_WS_BASE_URL=wss://api-staging.catalyst.example.com/ws

# Application Configuration
VITE_APP_NAME=Catalyst (Staging)
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=staging

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_DEBUG_LOGGING=true

# Chrome Extension Integration
VITE_EXTENSION_ID=your-staging-extension-id

# External Services
VITE_GOOGLE_ANALYTICS_ID=your-staging-ga-id
VITE_SENTRY_DSN=your-staging-sentry-dsn
