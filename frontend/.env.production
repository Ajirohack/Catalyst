# Production Environment Variables for Catalyst Frontend

# API Configuration - Update these for your production environment
VITE_API_BASE_URL=https://api.catalyst.example.com/api

# WebSocket Configuration
VITE_WS_BASE_URL=wss://api.catalyst.example.com/ws

# Application Configuration
VITE_APP_NAME=Catalyst
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_DEBUG_LOGGING=false

# Chrome Extension Integration
VITE_EXTENSION_ID=your-production-extension-id

# External Services
VITE_GOOGLE_ANALYTICS_ID=your-ga-id
VITE_SENTRY_DSN=your-sentry-dsn
