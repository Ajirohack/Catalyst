{
  "name": "catalyst-frontend",
  "version": "0.1.0",
  "private": true,
  "description": "Frontend for Catalyst System - Relationship coaching and analysis platform",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.10.0",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@mui/x-date-pickers": "^6.18.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "lucide-react": "^0.515.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.20.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.7.4",
    "styled-components": "^6.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^1.0.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^2.1.4",
    "yup": "^1.3.3",
    "zod": "^3.25.64"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:ci": "react-scripts test --ci --watchAll=false",
    "test:integration": "react-scripts test --testMatch=\"**/__integration__/**/*.test.{js,jsx}\"",
    "test:hooks": "react-scripts test --testMatch=\"**/hooks/*.test.{js,jsx}\"",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:component": "cypress run --component",
    "cypress:e2e": "cypress run --e2e",
    "cypress:accessibility": "cypress run --spec \"cypress/e2e/accessibility-tests.cy.js\"",
    "cypress:performance": "cypress run --spec \"cypress/e2e/performance-tests.cy.js\"",
    "cypress:api": "cypress run --spec \"cypress/e2e/api-tests.cy.js\"",
    "test:all": "npm run test:ci && npm run cypress:run",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.2",
    "@storybook/addon-essentials": "^7.6.2",
    "@storybook/addon-interactions": "^7.6.2",
    "@storybook/addon-links": "^7.6.2",
    "@storybook/preset-create-react-app": "^7.6.2",
    "@storybook/react": "^7.6.2",
    "@storybook/react-webpack5": "^7.6.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.3",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.54.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.0",
    "msw": "^1.3.3",
    "msw-storybook-addon": "^1.10.0",
    "postcss": "^8.5.5",
    "prettier": "^3.1.0",
    "storybook": "^7.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5"
  }
}
